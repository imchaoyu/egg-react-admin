{
  "name": "service",
  "version": "1.0.0",
  "description": "a seivice for system",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^2.35.0",
    "egg-cors": "^2.2.3",
    "egg-mysql": "^3.1.0",
    "egg-router-plus": "^2.0.0",
    "egg-scripts": "^2.17.0",
    "egg-sequelize": "^6.0.0",
    "egg-swagger-doc": "^2.3.2",
    "egg-validate": "^2.0.2",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3"
  },
  "devDependencies": {
    "autod": "^3.1.2",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.19.0",
    "egg-ci": "^1.19.0",
    "egg-mock": "^4.2.1",
    "egg-sequelize-auto": "^0.1.1",
    "eslint": "^7.32.0",
    "sequelize-cli": "^6.4.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-service",
    "stop": "egg-scripts stop --title=egg-server-service",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "auto-model": "egg-sequelize-auto -d eradmin-dev -h localhost -u root -p 3306 -x 1qazxsw2 -e mysql",
    "?db:*": "使用sequelize初始化,创建表,数据库变更",
    "db:config": "npx sequelize init:config && npx sequelize init:migrations",
    "db:g": "npx sequelize migration:generate --name=init-users",
    "db:p": "npx sequelize db:migrate"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "chaoyu",
  "license": "MIT"
}
